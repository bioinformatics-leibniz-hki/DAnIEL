  keywords           = {DNA Barcoding, Taxonomic/*methods, *Fermentation, *Metagenome, Saccharomyces cerevisiae/*genetics/metabolism, Wine/*microbiology, *metagenomics, *phylotyping, *wine, *yeast diversity},
  language           = {eng},
  linking-issn       = {2047-217X},
  location-id        = {10.1093/gigascience/gix040 [doi]},
  nlm-unique-id      = {101596872},
